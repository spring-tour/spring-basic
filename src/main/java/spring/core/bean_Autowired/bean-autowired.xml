<?xml version="1.0" encoding="UTF-8"?>
    <beans   xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config />
    <bean id="circle" class="spring.core.bean_Autowired.Circle"  />
    <bean id="c2" class="spring.core.bean_Autowired.Circle"  />
    <bean id="rectangle" class="spring.core.bean_Autowired.Rectangle"  />
 
    <!--shapeBox  -->
    <bean id="shapeBox" class="spring.core.bean_Autowired.ShareBox" />
 
</beans>